{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "no-empty-pattern": 0,
    "no-unused-vars": 0,
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
